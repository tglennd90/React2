{
  "name": "react2",
  "version": "1.0.0",
  "description": "React Homework #2 - Google Books API",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tglennd90/React2.git"
  },
  "author": "Tatum Daily",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tglennd90/React2/issues"
  },
  "homepage": "https://github.com/tglennd90/React2#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.20.1",
    "bootstrap": "^4.4.1",
    "colors": "^1.4.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.12",
    "morgan": "^1.10.0",
    "react-bootstrap": "^1.0.1"
  }
}
